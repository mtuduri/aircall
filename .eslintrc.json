{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": { "test": true, "expect": true },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "ignorePatterns": ["**/webpack.config.js"],
  "rules": {
    "react/react-in-jsx-scope": "off"
  }
}
